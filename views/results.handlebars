<div class="container">
<h3>My favourite Recipes</h3>

  <div class="col s12">
    <div class="card"> 

        <div class="card-content #424242 grey-text text-darken-3">
          <span class="card-title">Recipes:</span>
          
        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ｘ</button>
          </li>
          {{/each}}
        </ul>
   
         </div>
        <div class="card-action">
           <a href="/" class="#5d4037 brown-text text-darken-2">← Back To Home</a>
        </div>

    </div>
   </div>

</div>